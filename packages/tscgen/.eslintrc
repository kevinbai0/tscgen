{
  "extends": "./node_modules/@kevinbai/lint-rules/node/.eslintrc.js",
  "env": {
    "mocha": true
  },
  "plugins": [
    "eslint-plugin-tsdoc"
  ],
  "rules": {
    "@rushstack/typedef-var": 0,
    "@typescript-eslint/consistent-type-definitions": 0,
